{
    "name": "moinul/laravel-pdf-to-html",
    "version": "1.2.6",
    "description": "A Laravel package for converting PDF files to HTML, with support for text extraction, image extraction, and styling preservation. Features include PDF to HTML conversion, image extraction, CDN-hosted PDF support, and more.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "pdf",
        "html",
        "converter",
        "pdf-to-html",
        "image-extraction",
        "text-extraction"
    ],
    "autoload": {
        "psr-4": {
            "Moinul\\LaravelPdfToHtml\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Moinul\\LaravelPdfToHtml\\Tests\\": "tests"
        }
    },
    "authors": [
        {
            "name": "Moinul Islam",
            "email": "engrminul@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "ext-imagick": "*",
        "illuminate/support": "^7.0|^8.0|^9.0|^10.0|^11.0",
        "setasign/fpdf": "^1.8",
        "smalot/pdfparser": "^2.0",
        "tecnickcom/tcpdf": "^6.6",
        "mpdf/mpdf": "^8.2",
        "dompdf/dompdf": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0.1",
        "orchestra/testbench": "^9.0",
        "mockery/mockery": "^1.6"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Moinul\\LaravelPdfToHtml\\PdfToHtmlServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
} 