{
    "name": "moinul/laravel-pdf-to-html",
    "description": "A Laravel package to convert PDF files to HTML with preserved formatting and styling using poppler-utils",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "pdf",
        "html",
        "converter",
        "poppler-utils"
    ],
    "authors": [
        {
            "name": "Moinul Islam",
            "email": "engrminul@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^7.0|^8.0|^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0.1"
    },
    "autoload": {
        "psr-4": {
            "Moinul\\LaravelPdfToHtml\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Moinul\\LaravelPdfToHtml\\PdfToHtmlServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
