{
    "name": "moinul/laravel-pdf-to-html",
    "description": "A Laravel package that converts PDF files to HTML format with ease, supporting image extraction and CDN-hosted PDFs.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "pdf",
        "html",
        "converter",
        "pdf-to-html",
        "image-extraction",
        "cdn"
    ],
    "autoload": {
        "psr-4": {
            "Moinul\\LaravelPdfToHtml\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Moinul\\LaravelPdfToHtml\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Moinul",
            "email": "moinul@example.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/support": "^7.0|^8.0|^9.0|^10.0",
        "setasign/fpdf": "^1.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0|^9.0",
        "orchestra/testbench": "^5.0|^6.0|^7.0|^8.0",
        "mockery/mockery": "^1.3|^1.4"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Moinul\\LaravelPdfToHtml\\PdfToHtmlServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
} 