{
    "name": "moinul/laravel-pdf-to-html",
    "description": "A Laravel package to convert PDF files to HTML format",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Moinul\\LaravelPdfToHtml\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Moinul\\LaravelPdfToHtml\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Moinul",
            "email": "moinul@example.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0",
        "setasign/fpdf": "^1.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "^6.0|^7.0|^8.0",
        "mockery/mockery": "^1.4"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Moinul\\LaravelPdfToHtml\\PdfToHtmlServiceProvider"
            ]
        }
    }
} 